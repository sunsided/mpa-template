{
  "name": "@sunsided/mpa-template",
  "version": "0.0.0",
  "license": "EUPL-1.2",
  "author": "Markus Mayer <widemeadows@gmail.com>",
  "private": true,
  "type": "module",
  "keywords": ["template", "sveltekit", "sveltekit5", "yarn", "corepack"],
  "packageManager": "yarn@4.12.0",
  "homepage": "https://github.com/sunsided/mpa-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunsided/mpa-template.git"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "sync": "svelte-kit sync",
    "clean": "rm -rf .svelte-kit build .turbo node_modules",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:fix:files": "eslint --fix",
    "format": "node format-runner.mjs --write .",
    "format:check": "node format-runner.mjs --check .",
    "format:files": "node format-runner.mjs --write --ignore-unknown",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed --debug"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.1",
    "@unocss/extractor-svelte": "^66.5.10",
    "@unocss/preset-attributify": "^66.5.10",
    "@unocss/preset-icons": "^66.5.10",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.13.1",
    "jsdom": "^27.3.0",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.4",
    "svelte-eslint-parser": "^1.4.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "unocss": "^66.5.10",
    "vite": "^7.3.0",
    "vite-plugin-devtools-json": "^1.0.0",
    "vitest": "^4.0.16"
  }
}
